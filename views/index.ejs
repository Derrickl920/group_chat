<html>
<head>
	<title>LIANG CHAT</title>
   <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap-theme.min.css">
    <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <script type ="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src='script.js'></script>
    <link rel="stylesheet" type="text/css" href="style.css">
</head>
<body>

    <div id='login' class='container-fluid'>
        <h1 class='text-primary'>Groupchat App</h1>
        <form>
            <input id='login_name' class='form-control' type='text' placeholder='What is your name?'><br>
            <input id='login_submit' class='btn btn-primary' type='submit' value='Enter'>
        </form>
    </div>

    <div id='container'>
        <h1 class='text-primary'>Groupchat by Liang</h1>
        <div id='chat_box'></div>

    <form id="myform">
        <div class='row'>
        <input id='message' class='textinput form-control col-xs-6' type='text' name='submit' onclick='submitForm()'>
        <button class='btn btn-primary col-xs-1' id='chat_submit'>Send</button>
        <div>
    </form>
    </div>
</body>
</html>